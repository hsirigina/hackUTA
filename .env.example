# Auth0 Configuration
# Get these values from your Auth0 dashboard: https://manage.auth0.com/
AUTH0_DOMAIN=your-tenant.us.auth0.com
AUTH0_CLIENT_ID=your_client_id_here
AUTH0_CLIENT_SECRET=your_client_secret_here
AUTH0_CALLBACK_URL=http://localhost:8501/callback

# Application Settings
SECRET_KEY=your_secret_key_for_session_encryption
APP_URL=http://localhost:8501

# Database (optional - defaults to SQLite)
DATABASE_URL=sqlite:///driving_tracker.db

# Arduino API Configuration
ARDUINO_API_KEY=your_secret_api_key_for_arduino
API_PORT=5000
